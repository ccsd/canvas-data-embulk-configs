{% include '/canvas/data/config/community/csv_in' %}
{% include 'role_dim' %}
out:
  type: {{ env.EMBULK_SQLOUT }}
  driver_path: {{ env.EMBULK_DRIVER }}
  native_driver: {{ env.EMBULK_NATIVE_DRIVER }}
  native_driver: {{ env.EMBULK_NATIVE_DRIVER }}
  host: {{ env.EMBULK_HOST_TEST }}
  user: {{ env.EMBULK_USER_TEST }}
  password: {{ env.EMBULK_PASS_TEST }}
  port: {{ env.EMBULK_PORT }}
  database: {{ env.EMBULK_DB }}
  default_timezone: {{ env.EMBULK_TZ }}
  table: role_dim
  mode: replace
  insert_method: native
  column_options:
    id: {type: 'BIGINT NOT NULL PRIMARY KEY'}
    canvas_id: {type: 'INT NULL'}
    root_account_id: {type: 'BIGINT NULL'}
    account_id: {type: 'BIGINT NULL'}
    name: {type: 'VARCHAR(256) NULL'}
    base_role_type: {type: 'VARCHAR(256) NULL'}
    workflow_state: {type: 'VARCHAR(256) NULL'}
    created_at: {value_type: timestamp, timestamp_format: '%Y-%m-%d %H:%M:%S.%L'}
    updated_at: {value_type: timestamp, timestamp_format: '%Y-%m-%d %H:%M:%S.%L'}
    deleted_at: {value_type: timestamp, timestamp_format: '%Y-%m-%d %H:%M:%S.%L'}
  after_load: |
    ALTER TABLE "role_dim" ADD CONSTRAINT "CK_role_dim_workflow_state" CHECK ("workflow_state" IN ('active', 'built_in', 'inactive'));
    CREATE INDEX "role_dim_idx_root_account_id" ON "role_dim" ("root_account_id");
    CREATE INDEX "role_dim_idx_account_id" ON "role_dim" ("account_id");
    CREATE INDEX "role_dim_idx_workflow_state" ON "role_dim" ("workflow_state");